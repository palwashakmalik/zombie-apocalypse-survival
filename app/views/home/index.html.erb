  <%= video_tag "zoombie.mp4", autoplay: :autoplay, loop: :loop, mute: :mute, class: "home_video" %>
<div class="homewrapper ">
  <% if current_user %>
    <%if current_user.admin %>
    <%= render "admin"  %>
    <%else%>
      <div class=" d-flex flex-wrap " ><%= render @users%></div>
    <%end%>
    <%= link_to "Log Out", destroy_user_session_path, method: :delete  %>
    <%= link_to "Locations", locations_path  %>
  <% if  !current_user.infected?%>
    <%= link_to "Items", items_path  %>
    <%= link_to "Settings", edit_user_registration_path  %>
    <div class="d-flex" ><%= render @users%></div>
    <%end%>
  <%else%>
    <div >
      <h1 class="home" >Zoombies Apocalypse</h1>
      <%= link_to "Sign Up", new_user_registration_path, :class => "btn btn-secondary"  %>
      <%= link_to "Sign In", new_user_session_path, :class => "btn btn-warning" %>
    </div>
  <%end %>
</div>
